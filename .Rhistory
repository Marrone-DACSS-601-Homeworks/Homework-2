mystery=function(DF,positionsToUse,CountryColumn='Country'){
newDF=DF[,c(CountryColumn),drop = FALSE]
average='average'
newDF[,average]=apply(DF[,positionsToUse],1,mean,na.rm = TRUE)
return(newDF[,c(CountryColumn,average)])
}
